	 <QRY ID="1" COL_SIZE="3">
	 UPDATE ACTIA_AUDIT_PROPOSED_TRN SET GNUM_ISVALID=0,GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')),GNUM_LSTMOD_SEATID=? where NUM_AUDIT_PROPOSED_SLNO =? 
