<OPR ID="1" NAME="INSERT" BLOCK_SIZE="3" QRY_TOT_SIZE="3" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="16">
	  insert into ACTIA_AUDIT_PROPOSED_TRN (STR_AUDIT_START_YEAR,STR_AUDIT_END_YEAR,STR_AUDIT_BODY_CODE,NUM_DEPT_CODE,NUM_AUDIT_PROPOSED_SLNO,GSTR_REMARKS,GNUM_SEAT_ID,GNUM_LSTMOD_SEATID,GNUM_ISVALID,GNUM_HOSPITAL_CODE,GDT_LSTMOD_DATE,GDT_ENTRY_DATE,DT_AUDTING_TO_DATE,DT_AUDTING_FROM_DATE,STR_STATUS,STR_ACTION_TAKEN_BY) values (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)
