		 <QRY ID="1" COL_SIZE="10">
		insert into ACTIA_AUDIT_PROPOSED_DTL A (A.GNUM_HOSPITAL_CODE, A.STR_AUDIT_BODY_CODE, A.NUM_AUDIT_PROPOSED_SLNO,A.STR_PROPOSED_AUDIT_PARA, A.GNUM_ISVALID,A.GNUM_SEAT_ID, A.GDT_ENTRY_DATE, A.GDT_LSTMOD_DATE,A.GNUM_LSTMOD_SEATID, A.NUM_SL_NO ) values (?,?,?,?,?,?,?,?,?,?)
		 </QRY>
	 </BLOCK>
	
</OPR>

<OPR ID="4" NAME="DELETE" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
