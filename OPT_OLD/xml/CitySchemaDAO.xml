	 <QRY ID="1" COL_SIZE="6">
	 UPDATE PIST_CITY_MST T SET T.GDT_EFFECTIVE_TO=(TO_DATE(?,'DD-MON-YYYY')-1), T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CITY_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
