	 INSERT INTO PIST_CITY_MST (NUM_CITY_ID,STR_CITY_CODE,STR_CITY_NAME,STR_CITY_ST_NAME,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GSTR_REMARKS,GNUM_HOSPITAL_CODE,NUM_DIST_ID) values (?,?,?,?,?,?,?,?,?,?,?,?,?)   
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="4" NAME="INSERT" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="5">
	 UPDATE PIST_CITY_MST T SET  T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CITY_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
