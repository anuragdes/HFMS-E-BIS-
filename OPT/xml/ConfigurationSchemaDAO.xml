<?xml version="1.0" encoding="UTF-8"?>
<tns:Transaction MODULE_NAME="HR" PROCESS_NAME="ConfigurationMaster" xmlns:tns="http://www.example.org/transactionSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/transactionSchema transactionSchema.xsd ">

<OPR ID="1" NAME="INSERT" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="15">
	 INSERT INTO PIST_CONFIGURATION_MST (NUM_CONFIG_ID,STR_CONFIG_CODE,STR_CONFIG_NAME,STR_ATTRI_CODE,STR_ATTRI_VALUE,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GDT_LSTMOD_DATE,GNUM_LSTMOD_SEATID,GSTR_REMARKS,GNUM_HOSPITAL_CODE) values (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)   
	</QRY>
	</BLOCK>
</OPR>
 
 
    
<OPR ID="2" NAME="UPDATE" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="12">
	 UPDATE PIST_CONFIGURATION_MST SET STR_CONFIG_CODE=?,STR_CONFIG_NAME=?,STR_ATTRI_CODE=?,STR_ATTRI_VALUE=?,GDT_EFFECTIVE_FRM=?,GDT_EFFECTIVE_TO=?,GSTR_REMARKS=?,GNUM_ISVALID=?,GNUM_SEAT_ID=? WHERE GNUM_HOSPITAL_CODE=? AND  NUM_CONFIG_ID=? AND GNUM_SLNO=?
	 </QRY>
	</BLOCK>
</OPR>


<OPR ID="3" NAME="MODIFICATION" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="6">
	  UPDATE PIST_CONFIGURATION_MST T SET T.GDT_EFFECTIVE_TO=(TO_DATE(?,'DD-MON-YYYY')-1), T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CONFIG_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	  </QRY>
	 </BLOCK>
	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="13">
	 INSERT INTO PIST_CONFIGURATION_MST (NUM_CONFIG_ID,STR_CONFIG_CODE,STR_CONFIG_NAME,STR_ATTRI_CODE,STR_ATTRI_VALUE,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GSTR_REMARKS,GNUM_HOSPITAL_CODE) values (?,?,?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="4" NAME="INSERT" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="5">
	  UPDATE PIST_CONFIGURATION_MST T SET  T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CONFIG_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	  </QRY>
	 </BLOCK>
	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="13">
	  INSERT INTO PIST_CONFIGURATION_MST (NUM_CONFIG_ID,STR_CONFIG_CODE,STR_CONFIG_NAME,STR_ATTRI_CODE,STR_ATTRI_VALUE,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GSTR_REMARKS,GNUM_HOSPITAL_CODE) values (?,?,?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="6" NAME="DELETE" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="5">
	UPDATE PIST_CONFIGURATION_MST T  SET T.GNUM_ISVALID=0, T.GDT_LSTMOD_DATE =(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID = ?  WHERE    T.NUM_CONFIG_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE = ?
	</QRY>
	</BLOCK>
</OPR>
</tns:Transaction>


