<?xml version="1.0" encoding="UTF-8"?>
<tns:Transaction MODULE_NAME="COMITTEE" PROCESS_NAME="Bill Generation Transaction" xmlns:tns="http://www.example.org/transactionSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/transactionSchema transactionSchema.xsd ">
<OPR ID="1" NAME="INSERT" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="Y">
  <BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	<QRY ID="1" COL_SIZE="12">
	 INSERT INTO PISCGH_BL_GEN_TRN(GNUM_HOSPITAL_CODE,GNUM_SLNO,STR_BILL_NUMBER,DT_BILL_DATE,DT_PERIOD_FROM_DATE,DT_PERIOD_TO_DATE,NUM_TOTAL_AMOUNT,GDT_EFFECT_DATE,GDT_ENTRY_DATE,GNUM_ISVALID,GNUM_SEAT_ID,STR_REQ_TYPE) VALUES(?,?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
   </BLOCK>
   <BLOCK ID="2" QRY_SIZE="1" ROWS_INS="M" MANDATORY="Y">
	<QRY ID="1" COL_SIZE="13">
	 INSERT INTO PISCGH_BL_GEN_DTL(STR_REQ_TYPE,GNUM_HOSPITAL_CODE,GNUM_SLNO,STR_BILL_NUMBER,DT_BILL_DATE,GDT_EFFECT_DATE,GDT_ENTRY_DATE,GNUM_ISVALID,GNUM_SEAT_ID,STR_PAYMENT_MODE,NUM_CASH_AMOUNT,STR_CHEQUE_NUMBER,STR_BANK_DETAILS) VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
   </BLOCK>
</OPR>
<OPR ID="2" NAME="UPDATE" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
  <BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
    <QRY ID="1" COL_SIZE="4">
      UPDATE PISCGH_BL_GEN_TRN A SET  A.DT_BILL_DATE=? WHERE   A.STR_BILL_NUMBER=? AND A.GNUM_HOSPITAL_CODE=? AND A.STR_REQ_TYPE=?
    </QRY>
  </BLOCK>
  
</OPR>

<OPR ID="4" NAME="DELETE" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
  <BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
    <QRY ID="1" COL_SIZE="3">
      UPDATE PISCGH_BL_GEN_TRN A SET A.GNUM_ISVALID=0 WHERE A.STR_BILL_NUMBER=? and A.GNUM_HOSPITAL_CODE=? AND A.STR_REQ_TYPE=?
    </QRY>
  </BLOCK>
    <BLOCK ID="2" QRY_SIZE="1" ROWS_INS="M" MANDATORY="Y">
    <QRY ID="1" COL_SIZE="3">
      UPDATE PISCGH_BL_GEN_DTL A SET A.GNUM_ISVALID=0 WHERE A.STR_BILL_NUMBER=? and A.GNUM_HOSPITAL_CODE=? AND A.STR_REQ_TYPE=?
    </QRY>
  </BLOCK>
</OPR>




</tns:Transaction>