	 <QRY ID="1" COL_SIZE="6">
	  	  UPDATE PIST_CERTIFICATE_MST T SET T.GDT_EFFECTIVE_TO=(TO_DATE(?,'DD-MON-YYYY')-1), T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CERTF_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="12">
	 INSERT INTO PIST_CERTIFICATE_MST (NUM_CERTF_ID,STR_CERTF_CODE,STR_CERTF_NAME,STR_CERTF_DESC,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GSTR_REMARKS,GNUM_HOSPITAL_CODE) values (?,?,?,?,?,?,?,?,?,?,?,?)   
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="4" NAME="INSERT" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="6">
	  	  UPDATE PIST_CERTIFICATE_MST T SET  T.GDT_LSTMOD_DATE=(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID=?  WHERE T.NUM_CERTF_ID=? and T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="12">
	  INSERT INTO PIST_CERTIFICATE_MST (NUM_CERTF_ID,STR_CERTF_CODE,STR_CERTF_NAME,STR_CERTF_DESC,GNUM_SLNO,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GSTR_REMARKS,GNUM_HOSPITAL_CODE) values (?,?,?,?,?,?,?,?,?,?,?,?)   
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="6" NAME="DELETE" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
