	 <QRY ID="1" COL_SIZE="5">
	UPDATE PIST_CERTIFICATE_MST T  SET T.GNUM_ISVALID=0, T.GDT_LSTMOD_DATE =(TO_DATE(?,'DD-MON-YYYY')), T.GNUM_LSTMOD_SEATID = ?  WHERE    T.NUM_CERTF_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE = ?
	</QRY>
	</BLOCK>
</OPR>
