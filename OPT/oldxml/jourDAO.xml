<?xml version="1.0" encoding="UTF-8"?>
<tns:Transaction MODULE_NAME="BUDGET" PROCESS_NAME="journeyMaster" xmlns:tns="http://www.example.org/transactionSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/transactionSchema transactionSchema.xsd ">
  <OPR ID="1" NAME="INSERT" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
	 INSERT INTO TDA_JOUR_MODE_MST (NUM_JOUR_ID,NUM_JOUR_CODE,STR_JOUR_MODE,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GSTR_REMARKS,GNUM_SLNO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GNUM_HOSPITAL_CODE) VALUES (?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
	</BLOCK>
</OPR>


<OPR ID="3" NAME="CORRECTION" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
	  UPDATE TDA_JOUR_MODE_MST T SET T.NUM_JOUR_CODE=?, T.STR_JOUR_MODE=?, T.GDT_EFFECTIVE_FRM=TO_DATE(?,'DD-MON-YYYY'),T.GDT_EFFECTIVE_TO=TO_DATE(?,'DD-MON-YYYY'),T.GSTR_REMARKS=?,GDT_ENTRY_DATE=?,T.GNUM_ISVALID=?,GNUM_SEAT_ID=? WHERE NUM_JOUR_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=? 
	 </QRY>
	 </BLOCK>
</OPR>




<OPR ID="4" NAME="MODIFICATION" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="6">
	  UPDATE TDA_JOUR_MODE_MST T SET T.GDT_EFFECTIVE_TO=(TO_DATE(?,'DD-MON-YYYY')-1),GDT_LSTMOD_DATE=?,GNUM_LSTMOD_SEATID=? WHERE T.NUM_JOUR_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
