	 </BLOCK>
	 <BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="2" COL_SIZE="11">
	  INSERT INTO  TDA_JOUR_MODE_MST(NUM_JOUR_ID,NUM_JOUR_CODE,STR_JOUR_MODE,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GSTR_REMARKS,GNUM_SLNO,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GNUM_HOSPITAL_CODE) VALUES (?,?,?,?,?,?,?,?,?,?,?)
     </QRY>
   </BLOCK>	
</OPR>

	
<OPR ID="5" NAME="MODIFICATION" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="5">
	  UPDATE TDA_JOUR_MODE_MST T SET GDT_LSTMOD_DATE=?,GNUM_LSTMOD_SEATID=? WHERE NUM_JOUR_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
