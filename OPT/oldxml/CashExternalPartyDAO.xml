<?xml version="1.0" encoding="UTF-8"?>
<tns:Transaction MODULE_NAME="BUDGET" PROCESS_NAME="journeyMaster" xmlns:tns="http://www.example.org/transactionSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/transactionSchema transactionSchema.xsd ">
  <OPR ID="1" NAME="INSERT" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
	 INSERT INTO ACTCAS_EXTERNAL_PARTY_MST(STR_PARTY_ID,STR_PARTY_NAME,STR_PARTY_ADDRESS,STR_PARTY_TYPE,STR_HOSPITAL_NAME,STR_MOBILE_NO,STR_EMAIL,GNUM_ISVALID,GNUM_SEAT_ID,GDT_ENTRY_DATE,GNUM_HOSPITAL_CODE)VALUES (?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="2" NAME="CORRECTION" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="13">
	  UPDATE ACTCAS_EXTERNAL_PARTY_MST T set T.STR_PARTY_ID=?,T.STR_PARTY_NAME=?,T.STR_PARTY_ADDRESS=?,T.STR_PARTY_TYPE=?,T.STR_HOSPITAL_NAME=?,T.STR_MOBILE_NO=?,T.STR_EMAIL=?,T.GNUM_ISVALID=?,T.GNUM_SEAT_ID=?,T.GDT_ENTRY_DATE=?,T.GNUM_HOSPITAL_CODE=? where T.STR_PARTY_ID =? and T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
</OPR>

<OPR ID="3" NAME="DELETE" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="2">
	 UPDATE ACTCAS_EXTERNAL_PARTY_MST T set T.GNUM_ISVALID=0 where T.STR_PARTY_ID =? and T.GNUM_HOSPITAL_CODE=?
	</QRY>
	</BLOCK>
</OPR>

</tns:Transaction>


