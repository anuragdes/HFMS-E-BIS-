<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="6">
	  UPDATE ACT_TADA_TA_CATG_MST T SET T.GDT_EFFECTIVE_TO=(TO_DATE(?,'DD-MON-YYYY')-1),GDT_LSTMOD_DATE=?,GNUM_LSTMOD_SEATID=? WHERE T.NUM_CATG_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
  	  INSERT INTO ACT_TADA_TA_CATG_MST (NUM_CATG_ID,STR_CATG_CODE,STR_CATG_NAME,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GSTR_REMARKS,GNUM_SLNO,GNUM_ISVALID,GNUM_HOSPITAL_CODE,GDT_ENTRY_DATE,GNUM_SEAT_ID) VALUES (?,?,?,?,?,?,?,?,?,?,?)
     </QRY>
   </BLOCK>
</OPR>



<OPR ID="4" NAME="MODIFICATION" BLOCK_SIZE="2" QRY_TOT_SIZE="2" TRN_MGMT="N">
  <BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="5">
	  UPDATE ACT_TADA_TA_CATG_MST T SET GDT_LSTMOD_DATE=?,GNUM_LSTMOD_SEATID=? WHERE NUM_CATG_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=?
	 </QRY>
	 </BLOCK>
  
  	<BLOCK ID="2" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
