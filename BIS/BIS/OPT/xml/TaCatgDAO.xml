	 update ACT_TADA_TA_CATG_MST set GNUM_ISVALID=0 ,GDT_LSTMOD_DATE=?,GNUM_LSTMOD_SEATID=? where NUM_CATG_ID =? and GNUM_SLNO = ? and GNUM_HOSPITAL_CODE=?
