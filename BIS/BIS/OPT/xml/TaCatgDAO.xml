<?xml version="1.0" encoding="UTF-8"?>
<tns:Transaction MODULE_NAME="BUDGET" PROCESS_NAME="journeyMaster" xmlns:tns="http://www.example.org/transactionSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/transactionSchema transactionSchema.xsd ">
  <OPR ID="1" NAME="INSERT" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
	 INSERT INTO ACT_TADA_TA_CATG_MST (NUM_CATG_ID,STR_CATG_CODE,STR_CATG_NAME,GDT_EFFECTIVE_FRM,GDT_EFFECTIVE_TO,GSTR_REMARKS,GDT_ENTRY_DATE,GNUM_SLNO,GNUM_ISVALID,GNUM_SEAT_ID,GNUM_HOSPITAL_CODE) VALUES (?,?,?,?,?,?,?,?,?,?,?)
	</QRY>
	</BLOCK>
</OPR>

<OPR ID="2" NAME="CORRECTION" BLOCK_SIZE="1" QRY_TOT_SIZE="1" TRN_MGMT="N">
	<BLOCK ID="1" QRY_SIZE="1" ROWS_INS="S" MANDATORY="Y">
	 <QRY ID="1" COL_SIZE="11">
	
							
	  UPDATE ACT_TADA_TA_CATG_MST T SET  T.STR_CATG_CODE=?, T.STR_CATG_NAME=?, T.GDT_EFFECTIVE_FRM=TO_DATE(?,'DD-MON-YYYY'),T.GDT_EFFECTIVE_TO=TO_DATE(?,'DD-MON-YYYY'),T.GSTR_REMARKS=?,T.GDT_ENTRY_DATE=?,T.GNUM_ISVALID=?,T.GNUM_SEAT_ID=? WHERE T.GNUM_ISVALID =1 AND T.NUM_CATG_ID=? AND T.GNUM_SLNO=? AND T.GNUM_HOSPITAL_CODE=? 
	 </QRY>
	 </BLOCK>
</OPR>


